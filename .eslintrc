{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jquery": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "one-var": 0,
    "react/jsx-boolean-value": 0,
    "radix": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "react/no-multi-comp": 0,
    "no-console": 0
  },
  "globals": {
    "ddd": true,
    "dd": true
  }
}