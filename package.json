{
  "name": "react-responsive-slider",
  "version": "2.0.0",
  "description": "React Responsive Carousel Project",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./example/build",
    "init": "npm install",
    "prepublish": "rm -f ./index.js; webpack --config webpack.config.dist.js",
    "start": "npm run clean; webpack",
    "start:hard": "rm -rf ./node_modules; npm run init; npm run start",
    "watch": "npm run start; webpack-dev-server --hot --inline"
  },
  "contributors": [
    "Leandro Augusto Lemos <leandrowd@gmail.com>",
    "Bogdan Bodnaruk <babak.47031@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.3.13",
    "babel-core": "^6.3.13",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.2",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "react": "^0.14",
    "react-dom": "^0.14",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.10.1"
  },
  "npmName": "react-responsive-carousel",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "dependencies": {
    "react-easy-swipe": "0.0.3"
  }
}
